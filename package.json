{
  "name": "emmanuelchucks.com",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --cache .",
    "format": "prettier --write --cache .",
    "typecheck": "tsgo --noEmit",
    "check:all": "turbo run lint format typecheck",
    "generate:types": "wrangler types",
    "clean": "git clean -xdf"
