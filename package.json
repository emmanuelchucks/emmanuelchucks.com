    "#layouts/*": "./src/app/layouts/*.tsx",
    "#pages/*": "./src/app/pages/*.tsx",
    "#shared/*": "./src/app/shared/*.ts",
    "#utils/*": "./src/app/utils/*.ts"
  },
  "dependencies": {
    "@xstate/store": "^3.14.0",
    "date-fns": "^4.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-server-dom-webpack": "^19.2.3",
    "rwsdk": "^1.0.0-beta.41",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.19.0",
    "@cloudflare/workers-types": "^4.20251217.0",
    "@content-collections/core": "^0.13.0",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/vite": "^0.2.8",
    "@kasoa/eslint-config": "^0.0.8",
    "@kasoa/prettier-config": "^0.0.4",
    "@mdx-js/rollup": "^3.1.1",
    "@shikijs/rehype": "^3.20.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20251218.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "reading-time": "^1.5.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.6.3",
    "vite": "^7.3.0",
    "wrangler": "^4.56.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
